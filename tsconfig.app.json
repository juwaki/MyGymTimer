{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "target": "es5",

    "module": "esnext",
    "moduleResolution": "node",
   
    
    "typeRoots": [ "../node_modules/@types" ],
    "types": [ "core-js", "jasmine" ]
    
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}